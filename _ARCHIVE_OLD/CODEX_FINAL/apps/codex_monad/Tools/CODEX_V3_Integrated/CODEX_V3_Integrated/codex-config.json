    "Snippet": {
      "Enable": true,
      "MaxSizeKB": 64,
      "Extensions": [
        ".md",
        ".txt",
        ".rtf",
        ".json",
        ".yaml",
        ".yml",
        ".toml",
        ".ps1",
        ".sh",
        ".py",
        ".js",
        ".ts"
      ],
      "GroupBy": "theme",
      "Output": "E:\\\\CODEX_V3\\\\notes\\\\Snippets"
    },
    "AI": {
      "Enable": true,
      "Mode": "webhook",
      "Endpoint": "http://127.0.0.1:8765",
      "AuthHeader": "",
      "BatchSize": 24,
      "Fields": [
        "title",
        "summary",
        "keywords",
        "entities",
        "priority",
        "fractal_path"
      ]
    }
  },
  "PreRun": {
    "Enable": true,
    "MaxFiles": 200000,
    "MaxTextBytes": 1048576,
    "Similarity": {
      "ShingleSize": 7,
      "MinOverlapJaccard": 0.28,
      "SimHashHamming": 8
    },
    "Clustering": {
      "Enable": true,
      "MinClusterSize": 3,
      "Linkage": "single",
      "SimilarityMetric": "jaccard"
    },
    "ProposedLayout": {
      "Enable": true,
      "ApplyAutoLayout": true,
      "BucketRules": "theme_first"
    }
  },
  "Stages": {
    "Stage1_PreRun": {
      "Enabled": true,
      "Script": ".\\\\scripts\\\\pre-run\\\\pre_run.ps1"
    },
    "Stage2_Build": {
      "Enabled": true,
      "Script": ".\\\\scripts\\\\run-codex.ps1"
    }
  },
  "Routing": {
    "Enable": true,
    "Precedence": [
      "fractal_path",
      "themes",
      "din_code",
      "project",
      "language"
    ],
    "FractalPath": {
      "Enable": true,
      "Root": "E:\\\\CODEX_V3",
      "DefaultLeaf": "_unsorted"
